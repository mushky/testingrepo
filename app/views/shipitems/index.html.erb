<div class="container">
  <h1>Shipping Items</h1>
  <p>
    <button class="btn btn-default" type="button">    
      <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
      <%= link_to "Add Shipping Item", new_shipitem_path %>
    </button>
  </p>
  </h1>
  <% @shipitems.each do |shipitem| %>
  <table class="table table-hover">
    <td>
      <h4><%= shipitem.part%></h4>
      <strong>Serial:</strong> <%= shipitem.serial %><br>
      <strong>Description:</strong> <%= shipitem.description %><br>
      <strong>Date:</strong> <%= shipitem.date %><br>
      <strong>Quantity:</strong> <%= shipitem.quantity %><br>
      <strong>Shipping Number:</strong> <%= shipitem.shipping_number %><br>
      |<%= link_to 'View', shipitem_path(shipitem), class: "btn-sm btn-primary" %>|
      |<%= link_to 'Edit', edit_shipitem_path(shipitem), class: "btn-sm btn-warning"%>|
      |<%= link_to 'Delete', shipitem_path(shipitem), class: "btn-sm btn-danger", method: :delete, data: { confirm: "Are you sure?" } %>|
    </td>
  <% end %>
  </table>
<%= will_paginate @shipitem %>

