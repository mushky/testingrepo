<div class="container">
	<h3>Part # <%= @shipitem.part %></h3>
	<strong>Serial #:</strong>
	<%= @shipitem.serial %><br>
	<strong>Date:</strong>
	<%= @shipitem.date %><br>
	<strong>Description:</strong>
	<%= @shipitem.description %><br>
	<strong>Quantity:</strong>
	<%= @shipitem.quantity %><br>
	<strong>Shipping Number:</strong>
	<%= @shipitem.shipping_number %>
	<br>
	<%= link_to "Back", shipitems_path, class: "btn-sm btn-info" %>
 	<%= link_to 'Update', edit_shipitem_path(@shipitem), class: "btn-sm btn-warning" %>
  <%= link_to 'Remove', shipitem_path(@shipitem), class: "btn-sm btn-danger", method: :delete, data: { confirm: "Are you sure?" } %>
</div>